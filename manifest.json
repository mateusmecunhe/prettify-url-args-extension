{
    "name": "Prettify URL Arguments",
    "version": "0.1.0",
    "description": "Make your URL argumenrs prettier",
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "manifest_version": 2,
    "browser_action": {
        
        "default_title": "Pretty URL arguments"
    },
    "permissions": ["tabs", "<all_urls>"]
}